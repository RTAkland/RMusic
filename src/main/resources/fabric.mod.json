{
  "schemaVersion": 1,
  "id": "rmusic",
  "version": "${version}",
  "name": "Rmusic",
  "description": "在游戏内听音乐捏",
  "authors": [
    "RTAkland"
  ],
  "contact": {
    "website": "https://mod.rtast.cn/RMusic",
    "repo": "https://github.com/RTAkland/RMusic"
  },
  "license": "Apache-2.0",
  "icon": "assets/rmusic/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "cn.rtast.rmusic.client.RMusicClient"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "cn.rtast.rmusic.RMusic"
      }
    ]
  },
  "mixins": [
    "rmusic.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.10",
    "fabric": "*",
    "minecraft": "1.19.2",
    "fabric-language-kotlin": ">=1.9.0+kotlin.1.8.0"
  }
}
